{
  "manifest_version": 2,

  "name": "Vanadium",
  "description": "Open URLs upon request from other applications ('-remote').",
  "version": "0.1",

  "permissions": [
    "http://localhost:8636/*"
  ],
  "background": {
    "persistent": true, // Otherwise doesn't work on startup?
    "scripts": ["background.js"]
  }
}
